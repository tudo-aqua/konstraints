(set-logic QF_AX)
(declare-fun A () (Array Int Int))
(declare-fun value () Int)
(declare-fun index () Int)
(assert (= index 0))
(assert (= value 1))
(assert (= (select (store A index value) index) value))
(assert (= (select (store A index value) index) 1))
(assert (not (= (select (store A index value) index) 0)))
(check-sat)